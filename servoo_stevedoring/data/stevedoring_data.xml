<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_stevedoring_file" model="ir.sequence">
            <field name="name">Stevedoring File</field>
            <field name="code">servoo.stevedoring.file</field>
            <field name="prefix">SG</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_stevedoring_operation" model="ir.sequence">
            <field name="name">Stevedoring operation</field>
            <field name="code">servoo.stevedoring.operation</field>
            <field name="prefix">SG/OP/</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_stevedoring_outturn_report" model="ir.sequence">
            <field name="name">Stevedoring Outturn report</field>
            <field name="code">servoo.stevedoring.outturn.report</field>
            <field name="prefix">APM/OTR/</field>
            <field name="suffix">/%(y)s</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
